{
  "scripts": {
    "start-config": "webpack-dev-server --mode=development --port 9000 --env.isLocal=true",
    "start-react": "cd ./apps/react-app/ && webpack-dev-server --mode=development --port 9001",
    "start-vue": "cd ./apps/vue-app/ && vue-cli-service serve --port 9002",
    "start-svelte": "cd ./apps/svelte-app/ && rollup -c -w",
    "start:all": "concurrently --kill-others --kill-others-on-fail -p name --names \"config,react,vue,svelte\" \"npm run start-*\"",
    "lint": "eslint src --ext js,ts,tsx",
    "test": "jest",
    "format": "prettier --write './**'",
    "build": "webpack --mode=production"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently yarn:lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.7.4",
    "@babel/runtime": "^7.8.7",
    "@types/systemjs": "^6.1.0",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "html-webpack-plugin": "^4.0.4",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "jest-cli": "^25.2.7",
    "prettier": "^2.0.3",
    "pretty-quick": "^2.0.1",
    "serve": "^11.2.0",
    "ts-config-single-spa": "^1.7.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "@babel/preset-typescript": "^7.9.0",
    "eslint-config-ts-react-important-stuff": "^2.0.0",
    "typescript": "^3.8.3",
    "webpack-config-single-spa-ts": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-typescript": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "vue-cli-plugin-single-spa": "~1.1.0",
    "vue-template-compiler": "^2.6.11",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2"
  },
  "dependencies": {
    "single-spa": "^5.3.1",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/systemjs": "^6.1.0",
    "@types/webpack-env": "^1.15.1"
  }
}
